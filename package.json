{
  "name": "wilkins",
  "description": "A multi-transport async logging library for JavaScript",
  "version": "0.1.3",
  "author": "Enoah Netzach <f.castellarin@gmail.com>",
  "maintainers": [
    "Enoah Netzach <f.castellarin@gmail.com>"
  ],
  "repository": "https://github.com/EnoahNetzach/wilkins.git",
  "keywords": [
    "wilkins",
    "winston",
    "logging",
    "sysadmin",
    "tools"
  ],
  "dependencies": {
    "async": "~1.0.0",
    "chalk": "1.1.x",
    "cycle": "1.0.x",
    "eyes": "0.1.x",
    "isstream": "0.1.x",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "stack-trace": "0.0.x"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "cross-spawn-async": "^2.0.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-plugin-import": "^2.2.0",
    "hock": "1.x.x",
    "prettier-eslint-cli": "^3.1.2",
    "rimraf": "^2.6.1",
    "std-mocks": "~1.0.0",
    "vows": "0.7.x"
  },
  "main": "./lib/wilkins",
  "scripts": {
    "build": "babel src --out-dir lib",
    "eslint": "eslint src",
    "format": "prettier-eslint --write src/*.js src/**/*.js",
    "prepublish": "yarn format",
    "pretest": "rimraf test/fixtures/logs/*",
    "test": "vows --spec --isolate"
  },
  "engines": {
    "node": ">= 4"
  },
  "license": "MIT"
}
